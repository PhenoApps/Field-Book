<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="?attr/fb_button_color_normal" />
            <stroke
                android:width="2dp"
                android:color="?fb_border_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="?attr/fb_button_color_pressed" />
            <stroke
                android:width="2dp"
                android:color="?fb_border_color" />
        </shape>
    </item>
</selector>
