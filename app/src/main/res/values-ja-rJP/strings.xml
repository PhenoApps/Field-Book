<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Tutorial -->
    <string name="tutorial_dialog_title">チュートリアル</string>
    <string name="tutorial_dialog_description">チュートリアルを使用しますか？この設定は「高度な設定」で変更できます。</string>
    <string name="tutorial_main_infobars_title">インフォバー</string>
    <string name="tutorial_main_infobars_description">インフォバーは各エントリーの追加情報を表示し、ドロップダウンメニューで変更できます。</string>
    <string name="tutorial_main_traits_title">形質</string>
    <string name="tutorial_main_traits_description">小さい緑の矢印で形質間を移動します。</string>
    <string name="tutorial_main_traitlist_title">形質</string>
    <string name="tutorial_main_traitlist_description">現在収集している形質です。現在の形質を押すと調査可能な形質のリストを表示します。</string>
    <string name="tutorial_main_entries_title">エントリー</string>
    <string name="tutorial_main_entries_description">大きな黒い矢印でエントリー間を移動します。</string>
    <string name="tutorial_main_navinfo_title">項目の並べ替え</string>
    <string name="tutorial_main_navinfo_description">第1および第2オーダーを表示する場所です。</string>
    <string name="tutorial_main_datacollect_title">データ入力</string>
    <string name="tutorial_main_datacollect_description">データは画面のボタン領域で入力します。形質の種類によってレイアウトは異なります。</string>
    <string name="tutorial_main_na_title">欠測値</string>
    <string name="tutorial_main_na_description">欠測値にはNAボタンを使います。</string>
    <string name="tutorial_main_delete_title">データ削除</string>
    <string name="tutorial_main_delete_description">このボタンを使ってデータを削除します。</string>
    <string name="tutorial_main_search_title">検索</string>
    <string name="tutorial_main_search_description">検索アイコンでデータベース質問ができます。</string>
    <string name="tutorial_main_resources_title">資料室</string>
    <string name="tutorial_main_resources_description">資料室フォルダーにあるファイルの一覧を表示します。</string>
    <string name="tutorial_main_summary_title">総括</string>
    <string name="tutorial_main_summary_description">現在のエントリーの全てのデータの総括を表示します。</string>
    <string name="tutorial_main_lockdata_title">データ保護</string>
    <string name="tutorial_main_lockdata_description">現在のデータ入力を保護</string>
    <string name="tutorial_settings_fields_title">圃場</string>
    <string name="tutorial_settings_fields_description">圃場のインポートと管理を許可します。</string>
    <string name="tutorial_settings_fields_import">Field Bookを開始するには、最初に圃場ファイルをインポートします。</string>
    <string name="tutorial_settings_traits_title">形質</string>
    <string name="tutorial_settings_traits_description">形質の追加、インポートおよび管理することができます。</string>
    <string name="tutorial_settings_collect_title">データ収集</string>
    <string name="tutorial_settings_collect_description">表現型データを収集することができます。</string>
    <string name="tutorial_settings_settings_title">設定</string>
    <string name="tutorial_settings_settings_description">データ収集の追加設定をすることができます。</string>
    <string name="tutorial_settings_export_title">エクスポート</string>
    <string name="tutorial_settings_export_description">データは2種のフォーマットでエクスポートできます：データベースまたは表</string>
    <string name="tutorial_fields_add_title">圃場の追加</string>
    <string name="tutorial_fields_add_description">圃場ファイルのインポート</string>
    <string name="tutorial_fields_file_description">ファイルは3つのカラムを含む必要があります：ユニークID、第1オーダーおよび第2オーダー。第1/第2オーダーは圃場のナビゲーションに使用します。</string>
    <string name="tutorial_fields_select_title">選択圃場</string>
    <string name="tutorial_fields_select_description">圃場ファイルのインポート後にデータ収集を行う圃場を選択します。</string>
    <string name="tutorial_fields_delete_title">圃場の削除</string>
    <string name="tutorial_fields_delete_description">圃場を削除するには、圃場名の左側のメニューを開き削除を選択します。</string>
    <string name="tutorial_traits_add_title">形質の追加</string>
    <string name="tutorial_traits_add_description">形質の追加は新しい形質を作ることができます。異なる形質フォーマットは異なるデータ収集レイアウトになります。</string>
    <string name="tutorial_traits_format_title">形質フォーマット</string>
    <string name="tutorial_traits_format_description">異なる形質フォーマットは異なるアイコンで示されます。</string>
    <string name="tutorial_traits_visibility_title">形質の表示/非表示</string>
    <string name="tutorial_traits_visibility_description">チェックボックスをチェックすることでデータ収集をできないようにします。</string>
</resources>
